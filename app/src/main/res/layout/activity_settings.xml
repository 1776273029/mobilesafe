<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:text="设置中心"
        style="@style/TitleStytle"/>
    <com.test.android.mobilesafe.view.SettingItemView
        xmlns:mine="http://schemas.android.com/apk/res/com.test.android.mobilesafe"
        android:id="@+id/siv_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mine:destitle="自动更新设置"
        mine:deson="自动更新已开启"
        mine:desoff="自动更新已关闭"/>
    <com.test.android.mobilesafe.view.SettingItemView
        xmlns:mine="http://schemas.android.com/apk/res/com.test.android.mobilesafe"
        android:id="@+id/siv_call"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mine:destitle="电话归属地的显示设置"
        mine:deson="归属地显示已开启"
        mine:desoff="归属地显示已关闭"/>



</LinearLayout>
